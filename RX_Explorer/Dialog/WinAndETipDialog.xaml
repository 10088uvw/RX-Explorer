<Cls:QueueContentDialog xmlns:Cls="using:RX_Explorer.Class"
                        x:Class="RX_Explorer.Dialog.WinAndETipDialog"
                        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                        xmlns:local="using:RX_Explorer.Dialog"
                        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                        mc:Ignorable="d"
                        Title="{x:Bind Cls:Globalization.GetString('Common_Dialog_WarningTitle')}"
                        IsPrimaryButtonEnabled="False"
                        CloseButtonText="{x:Bind Cls:Globalization.GetString('Common_Dialog_CancelButton')}"
                        PrimaryButtonClick="QueueContentDialog_ButtonClick"
                        CloseButtonClick="QueueContentDialog_ButtonClick">

    <Grid>
        <TextBlock VerticalAlignment="Center"
                   TextWrapping="WrapWholeWords"
                   Text="{x:Bind Cls:Globalization.GetString('QueueDialog_BeforeInterceptWindowsETip_Content')}"/>
    </Grid>
</Cls:QueueContentDialog>
